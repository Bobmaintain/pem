{
  "compilerOptions": {
    "baseUrl": ".",
    "module": "commonjs",
    "target": "es2016",
    "lib": [
      "ES2016",
      "ES2016.Array.Include",
      "ES2021",
      "ES2021.Promise",
      "ES2021.String",
      "ES2021.WeakRef"
    ],
    "allowJs": true,
    "allowSyntheticDefaultImports": true,
    "declaration": true,
    "declarationMap": true,
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "importHelpers": true,
    "incremental": true,
    "inlineSources": true,
    "moduleResolution": "node",
    "newLine": "lf",
    "noImplicitAny": true,
    "noImplicitReturns": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "outDir": "dist",
    "preserveConstEnums": true,
    "removeComments": true,
    "noResolve": false,
    "resolveJsonModule": true,
    "skipLibCheck": true,
    "sourceMap": true,
    "strict": true,
    "typeRoots": [
      "./node_modules/@types",
      "./types"
    ],
    "types": [
      "jest",
      "node",
      "jest-extended"
    ]
  },
  "files": [
    "node_modules/jest-extended/types/index.d.ts",
    "types/globals.d.ts"
  ],
  "include": [
    "./**/*"
  ],
  "exclude": [
    "./node_modules/",
    "test/**/*",
    "./dist/"
  ]
}
