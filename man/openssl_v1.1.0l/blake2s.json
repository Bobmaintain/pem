              "_attributes": {
                "style": "margin-left:11%; margin-top: 1em"
              },
              "small": [
                {
                  "_value": "SHA-256"
                }
              ],
              "_values": [
                "To sign a file\nusing",
                "with binary file output:",
                "openssl dgst -sha256 -sign privatekey.pem -out\nsignature.sign file.txt"
              ],
              "br": [
                {}
              ]
            },
            {
              "_attributes": {
                "style": "margin-left:11%; margin-top: 1em"
              },
              "br": [
                {},
                {},
                {}
              ],
              "_values": [
                "To verify a\nsignature:",
                "openssl dgst -sha256 -verify publickey.pem \\",
                "-signature signature.sign \\",
                "file.txt"
              ]
            },
